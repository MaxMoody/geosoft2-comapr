<!-- New Project Dialogue -->
<div id="newProjectModal" class="modal fade" role="dialog">
    <div class="modal-dialog">
        <div class="modal-content">
            <div class="modal-header">
                <button type="button" class="close" data-dismiss="modal">&times;</button>
                <h4 class="modal-title">New Project</h4>
            </div>
            <div class="modal-body">
                <tr>
                    <td>Name</td>
                    <td><input type="text" placeholder="The name of your project" class="form-control"></td>
                </tr>
            </div>
            <div class="modal-footer">
                <button type="button" class="btn btn-default" data-dismiss="modal">Create Project</button>
            </div>
        </div>
    </div>
</div>

